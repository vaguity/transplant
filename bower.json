{
  "name": "transplant",
  "version": "1.0.12",
  "main": [
    "dist/assets/css/transplant.css"
  ],
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "gulp",
    "gulpfile.js",
    "dist/*.html",
    "src/css",
    "src/js",
    "src/lib",
    "src/scss/lib"
  ],
  "devDependencies": {
    "compass-breakpoint": "~2.5.0",
    "enquire": "~2.1.2",
    "html5-boilerplate": "~5.0.0",
    "jquery": "~1.11.2",
    "jquery-throttle-debounce": "git://github.com/cowboy/jquery-throttle-debounce.git#v1.1",
    "modernizr": "~2.8.3",
    "normalize-scss": "~3.0.1",
    "panelsnap": "~0.14.0",
    "susy": "~2.2.1"
  },
  "overrides": {
    "compass-breakpoint": {
      "main": "stylesheets/**"
    },
    "html5-boilerplate": {
      "main": "dist/css/main.css"
    },
    "jquery-throttle-debounce": {
      "main": "jquery.ba-throttle-debounce.min.js"
    },
    "modernizr": {
      "main": "modernizr.js"
    },
    "susy": {
      "main": "sass/**"
    }
  }
}
